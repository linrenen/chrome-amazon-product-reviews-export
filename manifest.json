{
  "name": "amazon",
  "version": "1.0",
  "description": "",
  "permissions": [
    "tabs",
    "cookies",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "<all_urls>",
    "declarativeContent",
    "storage",
    "proxy"
  ],
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.com/product-reviews"
      ],
      "js": [
        "jquery.js", "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2

}